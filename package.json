{
  "name": "allenjoseph.pe",
  "version": "1.0.0",
  "description": "Personal website",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allenjoseph/allenjoseph.pe.git"
  },
  "keywords": ["allenjoseph.pe"],
  "author": "Allen Joseph",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/allenjoseph/allenjoseph.pe/issues"
  },
  "homepage": "https://allenjoseph.pe",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "firebase": "^5.5.1",
    "jest-dom": "^1.12.1",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-scripts": "1.1.5",
    "react-testing-library": "^5.1.0"
  },
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.0.0-rc.14",
    "prettier": "1.14.2"
  }
}
